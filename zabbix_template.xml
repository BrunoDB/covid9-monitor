<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-03-11T12:40:37Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template - Covid9</template>
            <name>Coronavirus - Covid9 monitoring</name>
            <description>Getting cases from worldometers.info and plotting on Zabbix</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>COVID9 General Info</name>
                </application>
                <application>
                    <name>Covid Data</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Covid 9 Full Info</name>
                    <type>EXTERNAL</type>
                    <key>covid9_status.py</key>
                    <delay>30m</delay>
                    <history>1d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>COVID9 General Info</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>COVID9 - Affected Countries</name>
                    <type>EXTERNAL</type>
                    <key>covid9_affectedcountries.py</key>
                    <delay>1h</delay>
                    <lifetime>365d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - ActiveCases</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[ActiveCases, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.ActiveCases</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - NewCases</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[NewCases, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.NewCases</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - NewDeaths</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[NewDeaths, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.NewDeaths</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - Serious,Critical</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[Serious, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.[&quot;Serious,Critical&quot;]</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - TotalCases</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[TotalCases, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.TotalCases</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - TotalDeaths</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[TotalDeaths, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.TotalDeaths</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>[ {#COUNTRY_NAME} ] - TotalRecovered</name>
                            <type>DEPENDENT</type>
                            <key>covid9.data[TotalRecovered, {#COUNTRY_NAME}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Covid Data</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COUNTRY_NAME}.TotalRecovered</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>covid9_status.py</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
